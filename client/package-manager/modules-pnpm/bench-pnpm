#!/bin/zsh

rm -rf node_modules
rm -f pnpm-lock.yaml

node -v && pnpm -v
# холодная первая установка
pnpm store prune && time pnpm install
# горячая первая установка
pnpm store prune && rm -rf node_modules && time pnpm install
# переустановка
rm -rf node_modules && time pnpm install
# валидация
time pnpm install
